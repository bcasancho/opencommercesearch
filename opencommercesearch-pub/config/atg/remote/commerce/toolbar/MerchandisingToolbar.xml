    <toolbar-scope pane-id="browse" asset-area="searchQueryFacets"/>
    <toolbar-scope pane-id="browse" asset-area="searchRangeFacets"/>
    <toolbar-scope pane-id="browse" asset-area="searchDateFacets"/>
    <toolbar-scope pane-id="browse" asset-area="searchSynonymLists"/>
    <toolbar-scope pane-id="browse" asset-area="searchSynonymMappings"/>
    <operation-menu-item id="edit" divider="false" submenu="false"/>
    <operation-menu-item id="duplicate" divider="false" submenu="false"/>
    <operation-menu-item id="delete" divider="false" submenu="false"/>
    <operation-menu-item divider="true" submenu="false"/>
