    <toolbar-scope pane-id="browse" asset-area="searchBoostsBlocks"/>
    <toolbar-scope pane-id="browse" asset-area="searchRedirectRules"/>
    <toolbar-scope pane-id="browse" asset-area="searchBoosts"/>
